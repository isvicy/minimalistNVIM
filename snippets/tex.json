{

  "Skeleton": {
    "prefix": "skeleton",
    "body": [
      "\\documentclass{${1:type}}",
      " ",
      "${2:% preambles}",
      " ",
      "\\begin{document}",
      " ",
      "$0",
      " ",
      "\\end{document}"
    ],
    "description": "Create a document skeleton"
  },

  "Package": {
    "prefix": "package",
    "body": "\\usepackage{${1:package_name}}\n$0",
    "description": "Import a LaTeX package"
  },

  "Environment": {
    "prefix": "environment",
    "body": "\\begin{${1:env}}\n\t$0\n\\end{${1}}",
    "description": "Create the start and end of an environment"
  }
  
}
