{

  "Skeleton": {
    "prefix": "skeleton",
    "body": "\\documentclass{${1:type}}\n\n\\begin{document}\n\n$0\n\n\\end{document}",
    "description": "Create a document skeleton"
  },

  "Environment": {
    "prefix": "environment",
    "body": "\\begin{${1:env}}\n\t$0\n\\end{${1}}",
    "description": "Create the start and end of an environment"
  },

  "Bold": {
    "prefix": "bold",
    "body": "\\mathbf{${1:symbol}}$0",
    "description": "Create a bold symbol"
  }
  
}
